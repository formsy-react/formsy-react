{
  "name": "formsy-react",
  "version": "2.0.0-beta.2",
  "description": "A form input builder and validator for React",
  "keywords": [
    "form",
    "forms",
    "formsy",
    "react",
    "react-component",
    "validation"
  ],
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ]
  },
  "license": "MIT",
  "homepage": "https://github.com/formsy/formsy-react",
  "bugs": "https://github.com/formsy/formsy-react/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/formsy/formsy-react.git"
  },
  "author": "Christian Alfoni",
  "files": [
    "dist"
  ],
  "browser": "dist/formsy-react.umd.js",
  "main": "dist/formsy-react.cjs.js",
  "module": "dist/formsy-react.esm.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:js",
    "build:clean": "rm -r dist/* || true",
    "build:js": "rollup -c",
    "changelog": "auto-changelog",
    "deploy": "np",
    "format": "prettier --write \"**/*.{js,ts}\"",
    "lint": "eslint src/**/*.js",
    "preversion": "npm run lint",
    "test": "jest",
    "version": "npm run build && git add lib && git add release && npm run changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "form-data-to-object": "^0.2.0",
    "prop-types": "^15.5.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "auto-changelog": "^1.13.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babelrc-rollup": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "np": "^5.0.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.2.0 || ^16.0.0",
    "react-dom": "^16.2.0 || ^16.0.0",
    "rollup": "^1.16.2",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.4",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "sinon": "^7.3.2"
  },
  "peerDependencies": {
    "react": "^15.6.1 || ^16.0.0",
    "react-dom": "^15.6.1 || ^16.0.0"
  }
}
